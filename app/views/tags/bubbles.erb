<h1> Bubble Links </h1>

<ul id='links'>
  <li>
  <% p @links %>
  <% @links.each do |link| %>
      <% link.tags.each do |tag| %>
        <% if tag.tagname == "bubbles" %>
          Title: <%= link.title %>
          URL:   <%= link.url   %>
        <% end %>
      <% end %>
      <% end %>
    </li>
</ul>
